# Environment variables for the Vite Remix Plugin
# Copy this file to .env.local and fill in your actual values

# Backend API URLs
VITE_BACKEND_API_BASE=https://your-backend-api.com
VITE_API_BASE=https://your-api.com
VITE_FE_API_BASE=https://your-frontend-api.com

# Authentication
VITE_BEARER_TOKEN=your_bearer_token_here

# External Links
VITE_TELEGRAM_CHANNEL=https://t.me/your_telegram_channel
VITE_TALLY_FORM=https://tally.so/your_form_id 